#passwd=
#dropMem=
passwd = 
db=qservResult
unix_socket=%(QSERV_BASE_DIR)s/var/lib/mysql/mysql.sock
host= 
user=qsmaster
port=0

[partitioner]
#stripes=18
#substripes=10
#emptyChunkListFile=
substripes=%(QSERV_SUBSTRIPES)s
stripes=%(QSERV_STRIPES)s
emptyChunkListFile=%(QSERV_BASE_DIR)s/etc/emptyChunks.txt

[table]
#chunked=Source,ForcedSource
#subchunked=Object
#alloweddbs=LSST
chunked=Source,FaintSource
subchunked=Object
alloweddbs=LSST,qservTest_case01_qserv,qservTest_case02_qserv,qservTest_case03_qserv
partitionCols=Object:ra_PS,decl_PS,objectId;Source:raObject,declObject,objectId

