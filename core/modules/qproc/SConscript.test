# -*- python -*-
Import('env')
Import('libProducts')
Import('cacheObjs')

import os
import itertools

programs = []

# deps on other modules
modDeps = """css global log merger qana query parser proto mysql sql util xrdc""".split()
deps = itertools.chain(*map(lambda m: libProducts[m], modDeps))
deps = cacheObjs(env, deps, ".o")
deps = map(lambda i:i, deps) # un-chain
myDeps = filter(lambda i: not str(i).startswith("test"), env.Glob("*.cc"))
extDeps = "antlr boost_regex boost_thread pthread boost_system protobuf ssl crypto XrdPosix XrdClient XrdPosixPreload XrdUtils mysqlclient_r zookeeper_mt".split()
p = env.Program(["testCppParser.cc"] + myDeps + deps, LIBS=extDeps)
programs.append(p)

Return('programs')
