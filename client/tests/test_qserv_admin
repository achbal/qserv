show everything;
drop database db1a;
show everything;
drop database db1b;
drop database db2;
create database dbA client/examples/dbPartitioned.params;
drop database dbA;
create database dbA client/examples/dbPartitioned.params;
drop database dbA;
create database dbA client/examples/dbPartitioned.params;
